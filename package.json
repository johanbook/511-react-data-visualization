    "eslint": "8.35.0",
    "eslint-plugin-sonarjs": "0.18.0",
    "eslint-plugin-unicorn": "46.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "prettier": "2.8.4",
    "standard-version": "9.5.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write --prose-wrap always"
    ]
  }
